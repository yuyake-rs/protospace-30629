<main class="main">
  <div class="inner">
    <div class="greeting">
      <% if user_signed_in? %> 
         <%= "こんにちは、" %> 
         <%= link_to "#{current_user.name}", user_path(current_user), class: :greeting__link %>
         <%= "さん" %> 
      <% else %>
         <%= "ようこそ、ProtoSpaceへ" %> 
      <% end %>
    </div>
    <div class="card__wrapper">
       <%= render partial: 'prototype', collection: @prototypes %>
    </div>
  </div>
</main>
